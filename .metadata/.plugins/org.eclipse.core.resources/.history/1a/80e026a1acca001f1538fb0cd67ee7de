package com.arrays;

public class UpperCaseLowerCase {

	public static void main(String[] args) {
		String str="subbu,subbu";
		int mid=str.length()/2;
		String lowercase ="";
		String uppercase="";
		
		for(int i=0;i<str.length();i++) {
			lowercase=lowercase+Character.toLowerCase(str.charAt(i));
		}
	}

}
